[Unit]
Description=Holoscan Proxy Service
After=network.target

[Service]
Type=simple
EnvironmentFile=/etc/holoscan-proxy/env_file
ExecStart=/usr/bin/holoscan-proxy /etc/holoscan-proxy/config.yml
Restart=on-failure
RestartSec=2
User=nobody
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target

